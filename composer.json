{
  "name": "sjbr/sr-email-subscribe",
  "description": "Email address subscription for TYPO3.",
  "type": "typo3-cms-extension",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "TYPO3",
    "email",
    "address",
    "frontend",
    "subscription"
  ],
  "authors": [
    {
      "name": "Stanislas Rolland",
      "homepage": "http://www.sjbr.ca",
      "role": "Developer"
    }
  ],
  "homepage": "https://forge.typo3.org/projects/extension-sr_email_subscribe",
  "support": {
	"issues": "https://forge.typo3.org/projects/extension-sr_email_subscribe/issues"
  },
  "replace": {
	"typo3-ter/sr-email-subscribe": "self.version"
  },
  "require": {
    "typo3/cms-core": "^9.5 || ^10.4",
    "sjbr/sr-feuser-register": "^7.0",
    "sjbr/static-info-tables": "^6.9",
    "friendsoftypo3/tt-address": "^5.1 || ^5.2"
  },
  "suggest": {
  	 "sjbr/sr-freecap": "Allows to add a captcha to the subscription form.",
  	 "directmailteam/direct-mail" : "Allows to send newsletters to subscribers."
  },
  "autoload": {
    "psr-4": {
      "SJBR\\SrEmailSubscribe\\": "Classes/"
    }
  }
}